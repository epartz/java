 int numero1 = 1;
 int numero2 = 2;
	 boolean simNao = numero1 == numero2;
	   System.out.println("a variavel é igual ? " +simNao);
            simNao = numero1 != numero2;
        System.out.println("a variavel é diferente ? " +simNao);
            simNao = numero1 > numero2;
        System.out.println("a variavel é maior que "+ simNao);       
            simNao = numero1 >= numero2;
        System.out.println("a variavel é maior ou igual ? "+simNao);
            simNao = numero1 < numero2;
        System.out.println("a variavel é menor que ? "+simNao);
            simNao = numero1 <= numero2;
        System.out.println("a variavel é menor ou igual ? "+simNao);
